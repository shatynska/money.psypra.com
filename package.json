{
  "name": "cashbook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:types": "tsc --noEmit",
    "preview": "vite preview",
    "mock": "npx msw-auto-mock https://dro.psypra.com/api/swagger-json -o ./src/shared/api/generated/mock.ts --base-url https://dro.psypra.com --codes 200,201",
    "generate": "npm run mock && kubb generate --config kubb.config.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@kubb/cli": "^1.14.9",
    "@kubb/core": "^1.14.9",
    "@kubb/swagger": "^1.14.9",
    "@kubb/swagger-tanstack-query": "^1.14.9",
    "@kubb/swagger-ts": "^1.14.9",
    "@kubb/swagger-zod": "^1.14.9",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.8.6",
    "axios": "^1.6.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "localforage": "^1.10.0",
    "lucide-react": "^0.290.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.48.2",
    "react-lazily": "^0.9.2",
    "react-router-dom": "^6.15.0",
    "react-toastify": "^9.1.3",
    "sort-by": "^1.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^3.4.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.2.0",
    "msw": "^2.0.11",
    "msw-auto-mock": "^0.18.0",
    "msw-storybook-addon": "^2.0.0--canary.122.b3ed3b1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "react-test-renderer": "^18.2.0",
    "storybook": "^7.5.3",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-node-polyfills": "^0.16.0",
    "vitest": "^0.34.6"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{json,js,ts,jsx,tsx,html,yaml,yml}": [
      "prettier --write --ignore-unknown"
    ],
    "**/*.{ts,tsx}": [
      "tsc --noEmit"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet"
    ]
  }
}
